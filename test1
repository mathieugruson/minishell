int ft_cd(char **argv, int i)
	char	*path;
	char	*newpath;
	path = getcwd(path, 0);
		if (chdir(path) != 0)
		if (chdir(argv[i]) != 0)
	printf("c1\n");
	newpath = getcwd(newpath, 0);
